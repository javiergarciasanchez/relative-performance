<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="scale" displayName="Scale for 3D agents display" type="double" 
						defaultValue="40.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="perfSpTypes" displayName="Type of Perf Sp: RELATIVE, FIXED" type="java.lang.String" 
						defaultValue="RELATIVE;RELATIVE" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="shockProbability" displayName="Shock Probability" type="double" 
						defaultValue="3.0E-35" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="spaceSize" displayName="Size of Decision Space" type="int" 
						defaultValue="64" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="utilityWeights" displayName="Utility Weights" type="java.lang.String" 
						defaultValue="0.5;0.5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="shockStdDev" displayName="Shock Standard Deviation" type="double" 
						defaultValue="0.3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="iniMinWeight" displayName="Weight of min for ini min Performance" type="java.lang.String" 
						defaultValue="0.7;0.7" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="ruggedness" displayName="Ruggedness" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="potencialEntrants" displayName="Potencial Entrants" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="exitPercentile" displayName="Exit Percentile ( 1 to 100 )" type="java.lang.String" 
						defaultValue="10.0;10.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									
		/>
				<parameter name="noiseStdDev" displayName="Noise Standard Deviation" type="double" 
						defaultValue="0.1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="vertShift" displayName="Vertical Shift for 3D agents display" type="double" 
						defaultValue="-20.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="stopAt" displayName="Stop At" type="double" 
						defaultValue="100.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="dims3DHeight" displayName="3D Graph Height" type="double" 
						defaultValue="500.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="freqs" displayName="# Frecuencies in Fourier Series" type="int" 
						defaultValue="10" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


